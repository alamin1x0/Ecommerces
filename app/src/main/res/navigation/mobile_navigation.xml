<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.developeralamin.ecommerceapp.payment.paymenthome.PaymentHomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_payment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.developeralamin.ecommerceapp.payment.search.SearchFragment"
        android:label="Dashboard"
        tools:layout="@layout/fragment_search" />

    <dialog
        android:id="@+id/webViewDialog"
        android:name="com.developeralamin.ecommerceapp.payment.paymenthome.WebViewDialog"
        tools:layout="@layout/fragment_web_view" />

    <dialog
        android:id="@+id/bottomSheetDialog"
        android:name="com.developeralamin.ecommerceapp.payment.paymenthome.BottomSheetFragment"
        tools:layout="@layout/bottom_sheet_dialog" />

</navigation>